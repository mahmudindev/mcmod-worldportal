{
  "schemaVersion": 1,
  "id": "worldportal",
  "version": "${version}",
  "name": "World Portal",
  "description": "Create custom world portal to travel between dimensions.",
  "authors": [
    "mahmudindev"
  ],
  "contact": {
    "homepage": "https://github.com/mahmudindev",
    "sources": "https://github.com/mahmudindev/mcmod-worldportal"
  },
  "license": "ARR",
  "icon": "assets/worldportal/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.mahmudindev.mcmod.worldportal.fabric.WorldPortalFabric"
    ],
    "client": [
      "com.github.mahmudindev.mcmod.worldportal.fabric.client.WorldPortalFabricClient"
    ]
  },
  "mixins": [
    "worldportal.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.2",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "lithium:options": {
      "mixin.ai.poi.fast_portals": false
    }
  }
}
